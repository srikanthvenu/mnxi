<script>
    $(document).ready(function () {

        
   
        $('.date').datepicker({
            weekStart: 2,
            startDate: "01/10/2013",
            format: 'mm/dd/yyyy',

            forceParse: false,
            autoclose: true
        });
  
      

    });



 

</script>
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

 


